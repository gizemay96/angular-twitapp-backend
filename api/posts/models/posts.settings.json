{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "posts"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "text": {
      "type": "text"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "posts"
    },
    "comments": {
      "via": "post",
      "collection": "comments"
    }
  }
}
